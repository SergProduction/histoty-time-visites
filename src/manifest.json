{
  "name": "History time visit",
  "version": "0.1",
  "description": "History time visit chrome extension",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "activeTab",
    "history",
    "tabs"
  ],
  "background": {
    "service_worker": "./background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_idle",
      "js": ["./contentscript.js"]
    }
  ],
  "action": {
    "default_title": "collect stat visit urls and spend time each url"
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}

